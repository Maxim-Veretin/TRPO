<Window x:Class="TRPO_1._4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TRPO_1._4"
        mc:Ignorable="d"
        Title="MainWindow" Height="160" Width="390">
    <Grid>
        <Button x:Name="___Без_имени_" Content="Добавить" HorizontalAlignment="Left" Margin="29,45,0,0" VerticalAlignment="Top" Width="150" Click="Button_Click" Height="30"/>
        <TextBox x:Name="nm" HorizontalAlignment="Left" Height="25" Margin="10,10,0,0" TextWrapping="Wrap" Text="Введите простое число" VerticalAlignment="Top" Width="190" TextChanged="Nm_TextChanged"/>
        <ListBox x:Name="lb" HorizontalAlignment="Left" Height="95" Margin="210,10,0,0" VerticalAlignment="Top" Width="150"/>
        <Menu HorizontalAlignment="Left" Height="20" Margin="74,85,0,0" VerticalAlignment="Top" Width="60">
            <MenuItem Header="File" BorderThickness="1" IsChecked="True">
                <Button Content="Save" Click="Save"/>
                <Button Content="Load" Click="Load"/>
            </MenuItem>
        </Menu>

    </Grid>
</Window>